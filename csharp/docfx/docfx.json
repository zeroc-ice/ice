{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/*/*.csproj"],
          "exclude": [
            "src/IceDiscovery/**",
            "src/IceLocatorDiscovery/**"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "EnumSortOrder": "declaringOrder"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml"]
      },
      {
        "files": ["toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "sitemap": {
      "baseUrl": "https://doc.zeroc.com/api/ice/main/csharp/"
    },
    "overwrite": [
      {
        "files": ["overwrite/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern", "template"],
    "postProcessors": ["ExtractSearchIndex"],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Ice API",
      "_appName": "Docs",
      "_appFaviconPath": "images/favicon.ico",
      "_appLogoPath": "images/logo.png",
      "_appLogoUrl": "https://doc.zeroc.com/api/ice/main/csharp",
      "_appFooter": "Â© 2025 ZeroC",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/zeroc-ice/ice",
        "branch": "main",
        "apiSpecFolder": "docs-overwrite"
      }
    }
  }
}
