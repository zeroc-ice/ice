<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="../../../../../msbuild/ice.common.props" />
  <PropertyGroup>
    <AssemblyName>user</AssemblyName>
    <TargetFramework>net8.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    <OutputPath>.</OutputPath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <SliceCompile>
      <IncludeDirectories>../..</IncludeDirectories>
    </SliceCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <PackageReference Include="zeroc.icebuilder.msbuild" Version="5.0.9" />
  </ItemGroup>
  <ItemGroup>
    <SliceCompile Include="../../User.ice" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../core/core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="generated\" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="generated\User.cs">
      <SliceCompileSource>../../User.ice</SliceCompileSource>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../../../../../src/Ice/Ice.csproj" />
  </ItemGroup>
</Project>
