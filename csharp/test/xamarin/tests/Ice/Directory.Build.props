<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <Import Project="$(MSBuildThisFileDirectory)../../../../msbuild/ice.test.props" />
    <PropertyGroup>
        <AppTargetFramework Condition="'$(AppTargetFramework)' == ''">netcoreapp3.0</AppTargetFramework>
    </PropertyGroup>
    <Choose>
        <When Condition="'$(ICE_BIN_DIST)' == 'all'">
            <ItemGroup>
                <PackageReference Include="zeroc.ice.net" Version="3.7.3" />
            </ItemGroup>
        </When>
        <Otherwise>
            <ItemGroup>
                <Reference Include="$(MSBuildThisFileDirectory)../../../../lib/netstandard2.1/Ice.dll" />
                <Reference Include="$(MSBuildThisFileDirectory)../../../../lib/netstandard2.1/IceSSL.dll" />
            </ItemGroup>
        </Otherwise>
      </Choose>
      <ItemGroup>
        <ProjectReference Include="$(MSBuildThisFileDirectory)../TestCommon/testcommon.csproj" />
      </ItemGroup>
</Project>
