{
    "python.analysis.extraPaths": [
        "./scripts"
    ],
    "pylint.args": [
        "--rcfile=tox.ini"
    ],
    "C_Cpp.autoAddFileAssociations": false,
    "files.associations": {
        "Make.*": "makefile"
    },
    "C_Cpp.default.cppStandard": "c++20",
    "cSpell.words": [
        "Browsable",
        "istr",
        "ostr",
        "Upcall"
    ],
    "gradle.nestedProjects": true,
    "java.format.enabled": false
}
