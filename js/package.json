{
    "name": "@zeroc/ice",
    "version": "3.9.0-alpha0",
    "description": "Ice for JavaScript runtime",
    "author": "Zeroc, Inc.",
    "homepage": "https://zeroc.com",
    "repository": "https://github.com/zeroc-ice/ice.git",
    "license": "GPL-2.0",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=22.16.0"
    },
    "keywords": [
        "Ice",
        "rpc"
    ],
    "workspaces": [
        "packages/*"
    ],
    "type": "module",
    "exports": {
        ".": {
            "import": "./src/index.js",
            "types": "./src/index.d.ts"
        },
        "./slice2js": "./src/slice2js.js"
    },
    "browser": {
        "./src/Ice/FileLogger.js": "./src/Ice/FileLogger.browser.js",
        "./src/Ice/TcpTransceiver.js": "./src/Ice/TcpTransceiver.browser.js",
        "./src/Ice/TimerUtil.js": "./src/Ice/TimerUtil.browser.js"
    },
    "browserslist": "> 0.25%, not dead",
    "devDependencies": {
        "@eslint/js": "^9.32.0",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-strip": "^3.0.4",
        "@rollup/plugin-typescript": "^12.1.0",
        "@types/node": "^22.15.24",
        "c8": "^10.1.3",
        "del": "^8.0.0",
        "esbuild": "^0.27.3",
        "eslint": "^9.32.0",
        "globals": "^16.3.0",
        "gulp": "^5.0.1",
        "gulp-typescript": "^5.0.1",
        "http-server": "^14.1.1",
        "jshint": "^2.13.6",
        "prettier": "^3.7.0",
        "rollup": "^4.34.0",
        "ts-loader": "^9.5.4",
        "tslib": "^2.8.1",
        "typedoc": "^0.28.9",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0",
        "vinyl": "^3.0.1",
        "vite": "^6.3.0",
        "webpack": "^5.99.0"
    },
    "files": [
        "src/",
        "bin/",
        "slice/",
        "README.md"
    ],
    "scripts": {
        "build": "gulp",
        "build-doc": "node dts-bundle.js && npx typedoc --router structure dist/bundle.d.ts --disableGit --disableSources",
        "dist": "gulp dist",
        "clean": "gulp clean",
        "prepack": "node scripts/prepack.js",
        "test:unplugin": "node --test test/Slice/unplugin/*/*.test.js"
    }
}
